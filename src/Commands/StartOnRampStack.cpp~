#include "StartOnRampStack.h"

StartOnRampStack::StartOnRampStack() {
	ToContainerSpeed = 0.5;
	ToContainerTime = 0.3;
	ToToteSpeed = 0.5;
	ToToteTime = 0.3;
	RotateTime = 0;
	RotateSpeed = 0;
	ToAutoSpeed = -1;
	ToAutoTime = 2.7;
	// Add Commands here:
	// e.g. AddSequential(new Command1());
	//      AddSequential(new Command2());
	// these will run in order.

	// To run multiple commands at the same time,
	// use AddParallel()
	// e.g. AddParallel(new Command1());
	//      AddSequential(new Command2());
	// Command1 and Command2 will run in parallel.

	// A command group will require all of the subsystems that each member
	// would require.
	// e.g. if Command1 requires chassis, and Command2 requires arm,
	// a CommandGroup containing them would require both the chassis and the
	// arm.

	//AddSequential(new RotateX(180,0.4));


	AddSequential(new Release() );
	AddSequential(new DriveForTime(0, 	ToContainerSpeed  ,0 ,	ToContainerTime)); //Drive forward to grab container
	AddSequential(new Grab() ); //Grab container
	AddSequential(new LiftOne() ); //Lift to above one tote
	AddSequential(new DriveForTime (0,ToToteSpeed,0,ToToteTime)); //Drive to over tote
	AddSequential(new Release() ); //Drop container onto tote
	AddSequential(new LowerToBottom() );
	AddSequential(new Grab() );
	AddSequential(new DriveForTime(0,0,RotateSpeed,RotateTime) );
	AddSequential(new DriveForTime(0,ToAutoSpeed,0,ToAutoTime) );
	AddSequential(new Release() );
}
